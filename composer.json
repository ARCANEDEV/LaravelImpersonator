{
    "name":        "arcanedev/laravel-impersonator",
    "description": "This package allows you to impersonate as an other user.",
    "homepage":    "https://github.com/ARCANEDEV/LaravelImpersonator",
    "keywords":    [
        "arcanedev", "laravel", "auth", "impersonate", "impersonator", "users"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":                  ">=7.0",
        "arcanedev/support":    "~4.0",
        "arcanesoft/contracts": "~2.0"
    },
    "require-dev": {
        "phpunit/phpcov":  "~4.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelImpersonator\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelImpersonator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench-browser-kit=~3.0\" \"orchestra/database=~3.0\""
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\LaravelImpersonator\\ImpersonatorServiceProvider"
            ],
            "aliases": {
                "Impersonator": "Arcanedev\\LaravelImpersonator\\Facades\\Impersonator"
            }
        }
    }
}
